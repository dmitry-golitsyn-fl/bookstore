<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="HibernateBookstore" namespace="HibernateBookstore.Models">
  <class name="Book" table="books" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="long">
      <generator class="native" />
    </id>
    <property name="Name" />
    <property name="PublicationDate" />
    <property name="ISBN" />
  </class>
</hibernate-mapping> 
